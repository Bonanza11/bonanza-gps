{
  "version": 2,
  "routes": [
    { "src": "^/api/.*", "dest": "/api" },

    { "src": "^/$", "dest": "/public/index.html" },
    { "src": "^/success/?$", "dest": "/public/success.html" },
    { "src": "^/cancel/?$", "dest": "/public/cancel.html" },
    { "src": "^/admin/?$", "dest": "/public/admin.html" },

    { "src": "^/(?!api)(.*)$", "dest": "/public/$1" }
  ]
}
